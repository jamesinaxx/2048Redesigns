{
    "name": "2048 Cupcake Edition",
    "version": "0.1.2",
    "description": "– The Cupcakes version of 2048 game –",
    "permissions": [
        "declarativeContent"
    ],
    "background": {
        "scripts": [
            "source/extension/main.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
        },
        "default_title": "2048 Cupcake Edition"
    },
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "manifest_version": 2
}